@model IEnumerable<User>

<table class="table table-hover">
    <thead>
        <tr>
            <th scope="col">Name</th>
            <th scope="col">Title</th>
            <th scope="col">URL</th>
        </tr>
    </thead>
    @foreach (var item in Model)
    {
        <tr class="table">
            <td>@item.UserName</td>
            <td>@item.Email</td>
            <td><a href="@item.VideoLinkId" target="_blank">Watch</a></td>
        </tr>
    }
</table>