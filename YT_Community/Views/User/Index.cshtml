@model IEnumerable<User>

<table class="table table-hover">
    <thead>
        <tr>
            <th scope="col">Name</th>
            <th scope="col">Email</th>
            <th scope="col">Mobile Number</th>
        </tr>
    </thead>
    @foreach (var item in Model)
    {
        <tr class="table">
            <td>@item.UserName</td>
            <td>@item.Email</td>
            <td>@item.MobileNumber</td>
        </tr>
    }
</table>